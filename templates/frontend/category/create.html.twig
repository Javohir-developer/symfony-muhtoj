{% extends 'base.html.twig' %}
{% block body %}
    <div class="container">
        <h1>create Category</h1><br>

        {{ form_start(form) }}
            <div class="mb-3">
                    {{ form_widget(form.name) }}
                    <small class="text-danger">
                        {{ form_errors(form.name) }}
                    </small>
            </div>
            <button type="submit" class="btn btn-primary">Submit</button>
        {{ form_end(form) }}

    </div>

{% endblock %}